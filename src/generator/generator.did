type RejectionCode = variant {
  NoError;
  CanisterError;
  SysTransient;
  DestinationInvalid;
  Unknown;
  SysFatal;
  CanisterReject;
};
type Result = variant {
  Ok : record { text };
  Err : record { RejectionCode; text };
};
type Result_1 = variant { Ok : principal; Err : text };
type Result_2 = variant { Ok : Society; Err : text };
type Result_3 = variant { Ok; Err : record { RejectionCode; text } };
type Result_4 = variant { Ok; Err : text };
type Society = record {
  denizens_principals : vec principal;
  controllers : vec principal;
  owner : principal;
  name : text;
  canister_id : principal;
  description : text;
};
type SocietyArgs = record { name : text; description : text };
type SocietyInfos = record {
  is_denisen : bool;
  name : text;
  canister_id : principal;
  description : text;
  a_denizens : nat64;
};
service : {
  call_society_function : (text, text) -> (Result);
  create_society : (SocietyArgs) -> (Result_1);
  get_my_societies : () -> (vec SocietyInfos) query;
  get_societies : () -> (vec SocietyInfos) query;
  get_society : (text) -> (Result_2) query;
  set_society_name : (text, text) -> (Result_3);
  update_societies_wasm_code : () -> (Result_3);
  update_society : (text, SocietyArgs) -> (Result_4);
}
